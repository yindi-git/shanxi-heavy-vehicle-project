    <select id="selectRyPatentlPledgeList" parameterType="RyPatentlPledge" resultMap="RyPatentlPledgeResult">
        <include refid="selectRyPatentlPledgeVo"/>
        <where>  
            <if test="proposalId != null  and proposalId != ''"> and proposal_id = #{proposalId}</if>
            <if test="proposalName != null  and proposalName != ''"> and proposal_name like concat('%', #{proposalName}, '%')</if>
        </where>
    </select>
    
    <select id="selectRyPatentlPledgeById" parameterType="Long" resultMap="RyPatentlPledgeResult">
